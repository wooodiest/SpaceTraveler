<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginTop="5dp"
    android:layout_marginLeft="10dp"
    android:layout_marginRight="10dp"
    android:layout_marginBottom="5dp"
    tools:context=".Display4ExpeditionLogbookItemFragment">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <LinearLayout
            android:id="@+id/llExpeditionLookBookListReturn"
            android:layout_width="match_parent"
            android:orientation="vertical"
            android:padding="10dp"
            android:background="@drawable/rounded_transparent60_background_valuable_items"
            android:layout_height="wrap_content">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:background="@drawable/rounded_background_text_white">
                <ImageView
                    android:layout_width="30dp"
                    android:layout_marginLeft="10dp"
                    android:layout_gravity="center"
                    android:layout_height="30dp"
                    android:src="@drawable/fastbackwardbutton">

                </ImageView>
                <TextView
                    android:id="@+id/txvMainGameDisplayExpeditionLookBookItemTitle"
                    android:layout_width="match_parent"
                    android:gravity="center"
                    android:layout_marginRight="40dp"
                    android:paddingLeft="5dp"
                    android:paddingRight="5dp"
                    android:layout_height="wrap_content"
                    android:text="Completed Main Missions - 12/67"
                    android:background="@drawable/rounded_background_text_white"
                    android:padding="10dp">


                </TextView>
            </LinearLayout>



        </LinearLayout>
        <androidx.recyclerview.widget.RecyclerView
            android:layout_marginTop="5dp"
            android:id="@+id/rvExpeditionLookBookItem"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

        </androidx.recyclerview.widget.RecyclerView>
    </LinearLayout>


</FrameLayout>